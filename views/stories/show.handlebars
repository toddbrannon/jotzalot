<h1>Show Stories</h1>
<div class="row">
    <div class="col s12 m9">
        <h3>{{story.title}}</h3>
        <div class="card story">
            <div class="card-content">
                <span class="card-title">{{story.date}}</span>
                {{{story.body}}}
            </div>
        </div>
    </div>
    <div class="col s12 m3">
        <div class="card center-alight">
            <div class="card-content">
                <span class="card-title"></span>
            </div>
        </div>
    </div>
</div>